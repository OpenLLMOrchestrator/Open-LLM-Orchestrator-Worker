{
  "configVersion": "1.0",
  "worker": {
    "queueName": "core-task-queue",
    "strictBoot": false
  },
  "pipeline": {
    "defaultTimeoutSeconds": 30,
    "defaultAsyncCompletionPolicy": "ALL",
    "stagePlugins": {
      "ACCESS": "default",
      "MEMORY": "default",
      "RETRIEVAL": "default",
      "MODEL": "default"
    },
    "root": {
      "type": "GROUP",
      "executionMode": "SYNC",
      "children": [
        { "type": "STAGE", "name": "ACCESS", "timeoutSeconds": 10 },
        { "type": "STAGE", "name": "MEMORY" },
        { "type": "STAGE", "name": "MODEL", "timeoutSeconds": 120 }
      ]
    }
  }
}
